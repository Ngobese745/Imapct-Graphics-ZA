# Firebase Functions Environment Variables
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit .env to version control!

# SendGrid Configuration
SENDGRID_APIKEY=your_sendgrid_api_key_here

# App Check Configuration
APPCHECK_ENFORCE=false

# Functions Region Configuration
FUNCTIONS_REGION=europe-west1

# Yoco Payment Configuration
YOCO_SECRET=your_yoco_secret_key_here
YOCO_SECRET_KEY=your_yoco_secret_key_here

# Sender.net Configuration
SENDERNET_APIKEY=your_sendernet_api_key_here
